<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="1200.0" style="-fx-background-color: #4fc6d6;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.test.test.bifurcationgraph.BifurcationController">

    <!-- Первый VBox для Canvas -->
    <VBox prefHeight="600.0" prefWidth="1200.0" style="-fx-background-color: #ffffff;" VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
        </VBox.margin>
        <Canvas fx:id="canvas" height="600.0" width="1200.0" />
    </VBox>

</VBox>
